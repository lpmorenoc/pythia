{
    "name": "IITA_Cassava",
    "workDir": "work",
    "templateDir": "data/templates",
    "weatherDir": "data/weathers/nigeria/DSSAT_WTH",
    "sample": 2,
    "threads": 8,
    "cores": 16,
    "ghr_root": "data/base/GHR",
    "default_setup": {
        "template": "NGSP0000.CSX",
        "sites": "xy_from_vector::data/shapes/Nigeria_shapefile.shp",
        "startYear": 1984,
        "nyers": 32,
        "initialN": 5,
        "icin": 5,
        "icsw%": 0.25,
        "icrt": 5,
        "icres": 10,
        "icren": 0.8,
        "wsta":"lookup_wth::IITA::vector::data/shapes/Nigeria_shapefile.shp::CellID",
        "id_soil": "lookup_ghr::raster::data/rasters/iita_nigeria_ghr.tif",
        "ic_layers": "generate_ic_layers::$id_soil"
    },
    "dssat": {
        "executable": "C:/DSSAT47/dscsm047"
    },
    "default_analytics": {
        "per_pixel_prefix": "pp",
        "singleOutput": true,
        "columns": [
            "LATITUDE",
            "LONGITUDE",
            "HARVEST_AREA",
            "RUN_NAME",
            "CR",
            "PDAT",
            "MDAT",
            "HDAT",
            "HWAH",
            "GNAM",
            "CWAM",
            "PRCP"
        ]
    },
    "runs": [
        {
            "name": "cassava",
            "analysis_outfile": "outputs/unity/cassava.csv"
        }
    ]
}
